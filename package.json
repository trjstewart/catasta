{
  "name": "Catasta",
  "version": "0.0.0",
  "description": "Catasta is a NodeJS Project Framework",
  "repository": "https://github.com/trjstewart/catasta",
  "author": "Tylor Stewart <trjstewart@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:server",
    "build:server": "babel src -d out",
    "start": "npm run build && NODE_ENV=production forever out/index.js",
    "start:dev": "NODE_ENV=development nodemon src/index.js --exec babel-node"
  },
  "dependencies": {
    "babel-cli": "^6.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.1",
    "chalk": "^1.1.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "express-session": "^1.15.2",
    "express-validator": "^3.1.3",
    "favicon": "^0.0.2",
    "figlet": "^1.2.0",
    "morgan": "^1.8.1",
    "node-ses": "^2.0.5",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^6.1.5",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-beta11",
    "sequelize": "^3.30.4",
    "sequelize-cli": "^2.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "nodemon": "^1.11.0"
  }
}
